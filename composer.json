{
    "name": "kodekeep/haiku",
    "description": "Haiku Generators for PHP.",
    "keywords": [
        "kodekeep",
        "haiku"
    ],
    "homepage": "https://github.com/kodekeep/haiku",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.4",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\Haiku\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\Haiku\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
